<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.gdgand.rxjava.rxjavasample"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".GApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".queue.Queue1Activity" />
        <activity android:name=".queue.Queue2Activity" />

        <activity android:name=".scroll.FasterListActivity" />

        <service android:name=".fcm.message.MessageReceiverService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name=".fcm.token.FCMTokenService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>